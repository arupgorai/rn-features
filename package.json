{
  "name": "rnfeatures",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",

    "android:dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:qa": "react-native run-android --variant=qaDebug --appIdSuffix=qa",
    "android:prod": "react-native run-android --variant=prodDebug",

    "android:dev-release": "cd android && ./gradlew assembleDevRelease",
    "android:qa-release": "cd android && ./gradlew assembleQaRelease",
    "android:prod-release": "cd android && ./gradlew assembleProdRelease",

    "android:dev-debug": "cd android && ./gradlew assembleDevDebug",
    "android:qa-debug": "cd android && ./gradlew assembleQaDebug",
    "android:prod-debug": "cd android && ./gradlew assembleProdDebug",

    "createbundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -rf android/app/src/main/res/drawable-* android/app/src/main/res/raw && cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-config": "^1.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
